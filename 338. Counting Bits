方法一： 硬算
class Solution {
    public int[] countBits(int num) {
        int[] res = new int[num + 1];
        for (int i = 0; i <= num; i++) {
            res[i] = countOne(i);
        }
        return res;
    }
    public int countOne(int n){
        String binaryNum = Integer.toBinaryString(n);
        int count = 0;
        for (int i = 0; i < binaryNum.length(); i++) {
            if (binaryNum.charAt(i) == '1') count++;
        }
        return count;
    }
}

